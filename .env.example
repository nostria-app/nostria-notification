# Environment variables
NODE_ENV=development
PORT=3000

# Logging
LOG_LEVEL=info

# Nostria API Configuration
NOSTRIA_API_URL=http://localhost:3001
NOSTRIA_API_KEY=your-api-key-here

# Daemon Configuration
ENABLE_DAEMON=true

# Admin Configuration
# Comma-separated list of public keys that have admin access to list endpoints
ADMIN_PUBKEYS=PUBKEY1,PUBKEY2
